<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.hos.controller.PatientFormController">
    <stylesheets>
        <URL value="@/lk/ijse/hos/view/style/patientform.css" />
    </stylesheets>
   <children>
      <JFXTextField fx:id="txtpatientName" layoutX="54.0" layoutY="105.0" prefHeight="28.0" prefWidth="211.0" promptText="Patient Name" />
      <JFXTextField fx:id="txtpatientAge" layoutX="54.0" layoutY="160.0" prefHeight="20.0" prefWidth="211.0" promptText="Patient Age" text="Patient Age" />
      <JFXTextField fx:id="txtGender" layoutX="54.0" layoutY="206.0" prefHeight="28.0" prefWidth="211.0" promptText="Patient Gender" />
      <JFXTextField fx:id="txtpatientID" labelFloat="true" layoutX="54.0" layoutY="57.0" prefHeight="28.0" prefWidth="211.0" promptText="Patient ID" />
      <JFXTextField fx:id="txtAddress" labelFloat="true" layoutX="54.0" layoutY="256.0" prefHeight="28.0" prefWidth="211.0" promptText="Patient Address" />
      <JFXButton fx:id="btnSave" layoutX="347.0" layoutY="318.0" onAction="#OnSavebtnClick" prefHeight="26.0" prefWidth="84.0" text="Save" />
      <JFXButton fx:id="btnCancel" layoutX="461.0" layoutY="318.0" onAction="#OnCancelBtnClick" prefHeight="26.0" prefWidth="84.0" text="Cancel" />
      <ImageView fx:id="navigateHome" fitHeight="55.0" fitWidth="61.0" layoutX="531.0" layoutY="14.0" onMouseClicked="#onHomeimgClick" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assest/home-icon.png" />
         </image>
      </ImageView>
      <JFXButton fx:id="btnNewPatient" layoutX="313.0" layoutY="57.0" onAction="#OnNewPatientBtnClick" prefHeight="28.0" prefWidth="115.0" text="New Patient" />
   </children>
</AnchorPane>
